import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        

        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        if (num >= 50){
            return;
        }else {
            System.out.println("N[0] = " + num);
            for (int i = 1; i <= 10; i++) {
                num = num * 2;
                System.out.println("N[" + i + "] = " + num);
            }
        }
    }
}
